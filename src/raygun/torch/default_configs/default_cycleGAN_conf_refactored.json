{
    "framework": "torch",
    "system_type": "CycleGAN",
    "networks_types": {"gnet_type": "unet", "dnet_type": "patchdiscriminator"},
    "networks_kwargs": {"gnet_kwargs": {
                        "input_nc": 1,
                        "ngf": 64,
                        "padding_type": "same"
                    },
                    "dnet_kwargs": {
                        "input_nc": 1,
                        "downsampling_kw": 2, 
                        "kw": 3,
                        "n_layers": 3,
                        "ngf": 64},
                    "common_voxel_size": null,
                    "ndims": null},
    "model_type": "CycleModel",
    "model_kwargs": {"freeze_norms_at": null},
    "optimization_type": {"g_optim_type": "Adam", "d_optim_type": "Adam"},
    "optimization_kwargs": {"g_optim_kwargs": {"betas": [0.9, 0.999],
                                            "lr": 1e-5,
                                            "weight_decay": 0}, 
                            "d_optim_kwargs": {"betas": [0.9, 0.999],
                                            "lr": 1e-5,
                                            "weight_decay": 0}},
    "loss_type": "link",
    "loss_kwargs": {"g_lambda_dict": {"A": {
        "l1_loss": {"cycled": 10, "identity": 0.5},
        "gan_loss": {"fake": 1, "cycled": 0}}, 
        "B": {"l1_loss": {"cycled": 10, "identity": 0.5},
                    "gan_loss": {"fake": 1, "cycled": 0}}
            },
        "d_lambda_dict": {"A": {"real": 1, "fake": 1, "cycled": 0},
        "B": {"real": 1, "fake": 1, "cycled": 0}}},
    "datapipes_types": {"sampling_bottleneck": false,
                        "interp_order": null,
                        "side_length": 64,
                        "batch_size": 1,
                        "num_workers": 11,
                        "cache_size": 50,
                        "spawn_subprocess": false},
    "trainer_type": "CycleTrain",
    "trainer_kwargs": {"num_epochs": 20000,
                        "log_every": 20,
                        "save_every": 2000,
                        "model_path": "./models/",    
                        "model_name": "CycleGAN",
                        "tensorboard_path": "./tensorboard/",
                        "verbose": true,
                        "checkpoint": null,
                        "pretrain_gnet": false,
                        "random_seed": 42,
                        "snapshot_every": null}
}

